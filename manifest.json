{
  "manifest_version": 3,
  "name": "12ft Redirector",
  "version": "1.0",
  "description": "Bypass paywalls by redirecting to 12ft.io",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
